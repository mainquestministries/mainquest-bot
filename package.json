		"@sapphire/plugin-logger": "latest",
		"@sapphire/time-utilities": "latest",
		"@sapphire/type": "latest",
		"@sapphire/utilities": "latest",
		"colorette": "latest",
		"discord-api-types": "^0.37.28",
		"discord.js": "^14.7.1",
		"dotenv-cra": "^3.0.2",
		"node-cron": "^3.0.2",
		"prisma": "^4.4.0",
		"reflect-metadata": "^0.1.13"
	},
	"devDependencies": {
		"@sapphire/prettier-config": "latest",
		"@sapphire/ts-config": "latest",
		"@types/node": "latest",
